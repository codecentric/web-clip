{
  "manifest_version": 2,
  "name": "WebTrack - Keep track yourself",
  "short_name": "WebTrack",
  "version": "1.0.0",
  "description": "Your extensions description",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "WebTrack"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "web_accessible_resources": [],
  "options_page": "options.html"
}
