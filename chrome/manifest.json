{
  "manifest_version": 2,
  "name": "WebTrack - Keep track yourself",
  "short_name": "WebTrack",
  "version": "1.0.0",
  "description": "Your extensions description",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "WebTrack",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["contentScript.js"],
      "exclude_matches": ["https://angelo.veltens.org/*"]
    }
  ],
  "permissions": [],
  "web_accessible_resources": ["script.js"]
}